{
  "name": "dux-boilerplate",
  "version": "0.0.1",
  "description": "A simple React+Redux+PostCSS+Webpack Boilerplate",
  "main": "index.js",
  "scripts": {
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "test": "NODE_ENV=production mocha './client/modules/**/*.spec.js' --compilers js:babel-core/register",
    "test:watch": "npm test -- --watch",
    "clean": "rimraf dist",
    "start": "node server.js",
    "lint": "eslint client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lsgoulart/dux-boilerplate.git"
  },
  "keywords": [
    "React",
    "Redux",
    "PostCSS",
    "cssNext",
    "modular",
    "boilerplate",
    "webpack"
  ],
  "author": "Leonardo Goulart <leonardo.sgoulart@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lsgoulart/dux-boilerplate/issues"
  },
  "homepage": "https://github.com/lsgoulart/dux-boilerplate#readme",
  "dependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-react-display-name": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "classnames": "^2.2.5",
    "cors": "^2.7.1",
    "css-loader": "^0.23.1",
    "eslint": "^2.13.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-react": "^5.2.2",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "isomorphic-fetch": "^2.2.1",
    "node-sass": "^3.8.0",
    "postcss-import": "^8.1.2",
    "postcss-initial": "^1.5.1",
    "postcss-loader": "^0.9.1",
    "postcss-mixins": "^5.0.0",
    "postcss-nested": "^1.0.0",
    "postcss-simple-vars": "^3.0.0",
    "precss": "^1.4.0",
    "react": "^15.1.0",
    "react-addons-css-transition-group": "^15.1.0",
    "react-bootstrap": "^0.29.5",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.1",
    "react-router-redux": "^4.0.5",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.2.9",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "rimraf": "^2.5.2",
    "slugify": "^0.1.1",
    "style-loader": "^0.13.1",
    "uuid-js": "^0.7.5",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.11.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.0",
